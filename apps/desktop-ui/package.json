{
  "name": "@aktyn-assistant/desktop-ui",
  "version": "0.6.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "build": "cross-env yarn run typecheck && DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "dev": "cross-env BROWSER=none DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "format": "prettier --write src/**/*{.ts,.tsx}",
    "lint": "eslint src/**/*{.ts,.tsx} --fix",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "animejs": "^3.2.2",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.12",
    "@types/prismjs": "^1.26.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
