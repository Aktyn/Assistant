{
  "name": "aktyn-assistant",
  "version": "0.1.0",
  "private": true,
  "author": "RadosÅ‚aw Krajewski (Aktyn) <aktyn3@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aktyn/aktyn-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/Aktyn/aktyn-assistant/issues"
  },
  "homepage": "https://github.com/Aktyn/aktyn-assistants#readme",
  "workspaces": {
    "packages": [
      "packages/**",
      "apps/**"
    ]
  },
  "scripts": {
    "build:all": "turbo run build",
    "start:core": "turbo run start --filter='./apps/core'",
    "dev:all": "turbo run dev --continue --parallel",
    "dev:packages": "turbo run dev --continue --parallel --filter='./packages/*'",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint:all": "turbo run lint --single-package",
    "typecheck:all": "turbo run typecheck --single-package"
  },
  "devDependencies": {
    "prettier": "^3.3.0",
    "turbo": "^2.0.1"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@4.2.2"
}